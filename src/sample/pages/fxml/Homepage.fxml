<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Pagination?>
<?import javafx.stage.Screen?>

<?import javafx.scene.control.Label?>
    <StackPane  id="stackPane" fx:id="parent" fx:controller="sample.pages.Homepage" maxHeight="-Infinity"  maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000" prefWidth="1000" stylesheets="@../../css/homePage.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <fx:define>
        <Screen fx:factory="getPrimary" fx:id="screen"/>
    </fx:define>
    <children>
      <AnchorPane  prefHeight="1500"  prefWidth="1500">
            <Label id="labelTitle" layoutX="0.0" layoutY="-20.0"   mnemonicParsing="false"  text="S.B.O" prefHeight="150.0" prefWidth="200" />
               <children>
                  <Button fx:id="btn_create" styleClass="btnOperation" layoutX="400.0" layoutY="400.0" mnemonicParsing="false" text="Create" prefHeight="200.0" prefWidth="200.0" />
                  <Button fx:id="btn_existing" styleClass="btnOperation" layoutX="700.0" layoutY="400.0" mnemonicParsing="false" text="Existing" prefHeight="200.0" prefWidth="200.0"/>
                  <Button fx:id="btn_backup" styleClass="btnOperation" layoutX="1000.0" layoutY="400.0" mnemonicParsing="false" text="Backup" prefHeight="200.0" prefWidth="200.0"/>
                  <Button fx:id="btn_exit" styleClass="btnOperation" layoutX="1300.0" layoutY="400.0" mnemonicParsing="false" text="Exit" prefHeight="200.0" prefWidth="200.0"/>
                   <Pagination layoutX="0" layoutY="0" fx:id="pageManager" prefHeight="${screen.visualBounds.height}"  prefWidth="${screen.visualBounds.width}"></Pagination>
               </children>
  </AnchorPane>
    </children>
</StackPane>
